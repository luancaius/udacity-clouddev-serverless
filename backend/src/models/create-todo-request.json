{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "TodoItem",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9]+$"
        },
        "dueDate": {
            "type": "string"
        }
    },
    "required": [
        "name",
        "dueDate"
    ],
    "additionalProperties": false
}